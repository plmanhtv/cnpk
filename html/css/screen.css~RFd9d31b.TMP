/*
main font: 
main background color: #
main text color: #
hover text color: #
active text color: #
visited text color: #
*/

@font-face {
    font-family: 'agency';
    src: url('../fonts/4822485673-webfont.eot');
    src: url('../fonts/4822485673-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/4822485673-webfont.woff2') format('woff2'),
         url('../fonts/4822485673-webfont.woff') format('woff'),
         url('../fonts/4822485673-webfont.ttf') format('truetype'),
         url('../fonts/4822485673-webfont.svg#agency_fbregular') format('svg');
    font-weight: normal;
    font-style: normal;

}

/* reset & default */
html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend, input, textarea, select,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {margin: 0; padding: 0; border: 0; outline: 0; background: transparent; font-size: 100%; vertical-align: baseline;}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {display: block;}
body {background: url(../images/wrapper.gif) repeat-y center top;background-attachment: fixed; color: #141212;font-family: 'Open Sans', sans-serif; font-size: 13px; line-height: 1;}
textarea {font-family: inherit; overflow: auto; vertical-align: top; resize: none}
ol, ul {list-style: none;}
blockquote, q {quotes: none;}
blockquote:before, blockquote:after, q:before, q:after {content: ''; content: none;}
ins {text-decoration: none;}
del {text-decoration: line-through;}
abbr[title], dfn[title] {border-bottom: 1px dotted; cursor: help;}
table, caption, tbody, tfoot, thead, tr, th, td {margin: 0; padding: 0;}
table {border-spacing: 0;}
sup {vertical-align: text-top;}
sub {vertical-align: text-bottom;}

a {color: #000; text-decoration: none;}
a:hover {color: #f80606;}
a:active {color: #000;}
a:visited {color: #000;}
:focus {outline: 0;}

/* structure */
#container {width: 1000px; margin: 0px auto;vertical-align: top;}
	#header {width: 100%; position: relative;}
		#header .logo {font-size: 18px; margin: 20px; display: block;}
		.lst-navigation {width: 100%; position: relative;height: 62px; background: #F7F7F7; margin-top: -2px; }
			#nav {height: 62px; background: #F7F7F7; position: absolute; left: 50%; margin-left:-300px;}
				#nav li {float: left; width: 132px; height: 62px; position: relative;}
				#nav li.sep {width: 1px; height: 62px; background-color: #cbcbcb;}
				#nav li .base {width: 168px; height: 64px; position: absolute; top: 2px; left: -18px;-webkit-transition: all 94ms ease-out;
transition: all 94ms ease-out; -moz-transition: all 94ms ease-out; -o-transition: all 94ms ease-out; opacity: 0;filter: alpha(opacity=0); background: url(../images/navigation_item.png) no-repeat left top;}
				#nav li:hover .base {top: -2px;opacity: 1;}
				#nav li .title {position: absolute; top: 0; left: 0; width: 132px; height: 65px;}
					#nav li  a {font-size: 17px; font-family: 'Open Sans', sans-serif; width: 132px; height: 12px; margin-top: 20px; text-align: center; display: block; -webkit-transition: all 94ms ease-out;
transition: all 94ms ease-out; -moz-transition: all 94ms ease-out; -o-transition: all 94ms ease-out; opacity: 0.99}	
	#main {width: 100%; background: #fff;}
		#sidebar {float: right; width: 220px; min-height: 530px; background: #1b1b1b; overflow: hidden; padding: 20px 15px 20px 15px;}
		#content {float: left; width: 730px; overflow: hidden; padding: 20px 0 10px 20px;}
	#footer {padding: 20px 40px 20px 70px; background: ##2a2a2a; overflow: hidden;}
	.block-footer {float: left; width: auto; border-left: 1px solid #141212; color: #fff; min-height: 160px; margin-left: 100px; padding-left: 100px; }
	#footer .block-footer:first-child {border-left: none; margin-left: 0; padding-left: 0;}
	.block-footer a {color: #fff; font-size: 12px;}
	.block-footer a:hover {color: #f80606;}
	.block-footer h3 {font-size: 17px; font-weight: normal; margin-bottom: 20px; color: #D3E8E8;}
	.block-footer ul li {margin-bottom: 10px;}

/* page */
.lst-game .game_list_block {float: left;}
.lst-game .game_list_block li {float: left; width: 182px; margin-bottom: 8px; position: relative; cursor: pointer; overflow: hidden;}
.lst-game .game_list_block li a > img {width: 95%; box-shadow: 0 0 5px #333;	}
.layer-mask {height: 110px; position: absolute; overflow: hidden; top: 0; left: 0; opacity: 0; background-color: rgba(33,33,33, 0.7); -webkit-transition: all 0.4s ease-in-out; -moz-transition: all 0.4s ease-in-out; -o-transition: all 0.4s ease-in-out; -ms-transition: all 0.4s ease-in-out; transition: all 0.4s ease-in-out; width: 172px;}
.layer-mask p {webkit-transform: translateY(100px);-moz-transform: translateY(100px);-o-transform: translateY(100px);
-ms-transform: translateY(100px);transform: translateY(100px);filter: alpha(opacity=0);opacity: 0;-webkit-transition: all 0.2s linear;-moz-transition: all 0.2s linear;-o-transition: all 0.2s linear;-ms-transition: all 0.2s linear;transition: all 0.2s linear;position: relative;color: #fff;padding: 10px;text-align: center;text-transform: none;}
.layer-mask .info {filter: alpha(opacity=0); opacity: 0;-webkit-transition: all 0.2s ease-in-out;-moz-transition: all 0.2s ease-in-out;-o-transition: all 0.2s ease-in-out;-ms-transition: all 0.2s ease-in-out;transition: all 0.2s ease-in-out;
display: block;text-decoration: none;padding: 7px 15px;background: #000;color: #f80606;text-transform: uppercase;-webkit-box-shadow: 0 0 1px #000;-moz-box-shadow: 0 0 1px #000;box-shadow: 0 0 1px #000; text-align: center;}
.lst-game .game_list_block li:hover .layer-mask {opacity: 1;}
.lst-game .game_list_block li:hover a.info,
.lst-game .game_list_block li:hover p {filter: alpha(opacity=100); opacity: 1; -webkit-transform: translateY(0px)-moz-transform: translateY(0px);
-o-transform: translateY(0px);
-ms-transform: translateY(0px);
transform: translateY(0px);}
.lst-game h2 {background: #333333;
box-shadow: 1px 0px 1px #141212,0px 1px 1px #141212,2px 1px 1px #333,1px 2px 1px #141212,3px 2px 1px #141212,2px 3px 1px #333,4px 3px 1px #141212,3px 4px 1px #333,5px 4px 1px #141212,4px 5px 1px #333,6px 5px 1px #141212;
padding: 7px 0 5px 20px;color: #fff; width: 693px;
margin-bottom: 25px;}
/* page-1 */
.block-registry {color: #fff;font-family: 'EB Garamond', serif; font-size: 15px; margin-bottom: 20px; }
.block-registry input[type="text"] {background: #fff; height: 15px; padding: 5px; width: 95%; margin: 10px 0 0;}
.block-registry input[type="submit"] {background: #fff; padding: 5px 0; margin-top: 5px; width: 100%; text-align: center; }
.block-registry input[type="submit"]:hover {cursor: pointer; background: #141414; color: #fff;}
.block-registry h2 {font-family: 'EB Garamond', serif; color: #f80606; font-size: 18px; text-transform: uppercase; margin-bottom: 10px; background: #fff; padding: 13px 10px; width: 108%; margin-left: -20px}
.block-registry p {margin-bottom: 10px;}
.block-registry ul li {margin-bottom: 10px;}
.block-registry ul li a {color: #fff;font-family: 'EB Garamond', serif; font-size: 15px;}
.block-registry a.dangky {color: #f80606; text-decoration: underline;}
/* page-2 */
.page-content {background: url(../images/bg-main-page.png) repeat 0 0;}
.top-nav {overflow: hidden; padding: 12px 0 12px 20px;}
.top-nav li {background: url(../images/icon-card.png) no-repeat 0 0;  float: left; margin-right: 70px; padding: 8px 0 12px 50px;}
.top-nav li a {font: 24px/26px agency, Arial, Helvetica, sans-serif; color: #fff;}
.block-user {height: 60px; background: #f7f7f7;}
.block-user .left {float: left; width: 240px; background: url(../images/icon-dot.png) repeat-y right 0; position: relative; height: 60px;}
.block-user .left > a { display: block; border: 1px solid #187c91; border-radius: 2px; -moz-border-radius: 2px; -border-radius: 2px; -o-border-radius: 2px; -webkit-border-radius: 2px; float: left; position: absolute; top: 5px; left: 5px; width: 64px; height: 63px; z-index: 1; pa}
.block-user .left p {padding: 4px 0 0 80px;}
.block-user .left p span {display: block;}
.block-user .left p span.price {background: url(../images/icon-dollar.png) no-repeat 0 4px; padding: 10px 0 10px 30px; color: #f70709; font-weight: bold; margin-top: 5px;}
.block-user .right {float: right; width: 324px; background: url(../images/icon-dot.png) repeat-y 0 0;}
.block-user .right a {display: block; width: 48px; height: 47px; float: left; margin: 6px 0 7px 14px;}
.block-user .right a.user-add {background: url(../images/icon-add-user.png) no-repeat 0 0;}
.block-user .right a.user-add:hover {background: url(../images/icon-user-hover.png) no-repeat 0 0;}
.block-user .right a.sound {background: url(../images/icon-sound.png) no-repeat 0 0;}
.block-user .right a.sound:hover {background: url(../images/icon-sound-hover.png) no-repeat 0 0;}
.block-user .right a.picture {background: url(../images/icon-picture.png) no-repeat 0 0;}
.block-user .right a.picture:hover {background: url(../images/icon-picture-hover.png) no-repeat 0 0;}
.block-user .right a.around {background: url(../images/icon-around.png) no-repeat 0 0;}
.block-user .right a.around:hover {background: url(../images/icon-around-hover.png) no-repeat 0 0;}
.block-user .right a.dollar {background:url(../images/icon-dollar-02.png) no-repeat 0 0;}
.block-user .right a.dollar:hover {background:url(../images/icon-dollar-hover.png) no-repeat 0 0;}
.main-content {padding: 20px; overflow: hidden;}
.main-content .left {float: left; width: 244px; margin-right: 15px; background: #187c91;}
.main-content .title { color: #333; font-family: 'EB Garamond', serif; font-weight: 500; font-size: 20px; padding: 10px 0 10px 40px;}
.main-content .left .title {background: url(../images/icon-wating.png) no-repeat 4px 5px #fff;}